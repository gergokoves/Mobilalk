<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KezdolapActivity">





    <TextView
        android:id="@+id/kezdolapTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/register_margin_top"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Fürdőruhák"
        android:textAlignment="center"
        android:textColor="@color/purple_700"
        android:textSize="30sp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/kezdolapNoiruhaId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Női fürdőruhák "
        android:onClick="noiRuhak"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.414" />

    <Button
        android:id="@+id/kezdolapFerfiruhaId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Férfi fürdőruhák"
        android:onClick="ferfiRuha"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kezdolapNoiruhaId"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>